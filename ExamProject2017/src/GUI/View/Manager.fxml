                  </TextField>
                  <ComboBox prefWidth="150.0" promptText="Search..." />
               </children>
            </GridPane>
         </top>
         <left>
            <GridPane prefHeight="502.0" prefWidth="480.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <TableView prefHeight="502.0" prefWidth="466.0">
                    <columns>
                      <TableColumn prefWidth="150.0" text="First Name" />
