                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="btnAddManager" mnemonicParsing="false" text="Add Manager" />
                  <Button fx:id="btnRemove" mnemonicParsing="false" text="Remove Manager" GridPane.rowIndex="1" />
                  <Button fx:id="btnEdit" mnemonicParsing="false" text="Edit Manager" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </right>
      </BorderPane>
